<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<!-- <?import br.com.agencialove.tpa.utils.MaskField?> -->

<AnchorPane prefHeight="778.0" prefWidth="1024.0" styleClass="tela-inicial" stylesheets="@styles/style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.agencialove.tpa.view.SearchZipController">
    <children>
        <TableView fx:id="tableView" layoutX="12.0" layoutY="296.0" prefHeight="192.0" prefWidth="1001.0" stylesheets="@styles/style.css">
        	<columnResizePolicy>
            	<TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
        	</columnResizePolicy>
        	<placeholder>
       			<Label text="" />
    		</placeholder>
	        <columns>
				<TableColumn id="tbcolStreet" prefWidth="300.0" resizable="false" text="Logradouro">
					 <cellValueFactory><PropertyValueFactory property="street" /></cellValueFactory>
				</TableColumn>
				<TableColumn id="tbcolQuarter" prefWidth="250.0" resizable="false" text="Bairro">
					<cellValueFactory><PropertyValueFactory property="neighborhood" /></cellValueFactory>
				</TableColumn>
				<TableColumn id="tbcolCity" prefWidth="100.0" text="Cidade">
					<cellValueFactory><PropertyValueFactory property="city" /></cellValueFactory>
				</TableColumn>
				<TableColumn id="tbcolState" prefWidth="100.0" resizable="false" text="UF">
					<cellValueFactory><PropertyValueFactory property="state" /></cellValueFactory>
				</TableColumn>
				<TableColumn id="tbcolZip" prefWidth="110.0" resizable="false" text="CEP">
					<cellValueFactory><PropertyValueFactory property="zip" /></cellValueFactory>
				</TableColumn>
			</columns> 
        </TableView>
        <Label fx:id="titleLabel" alignment="CENTER" layoutX="12.0" layoutY="14.0" prefHeight="44.0" prefWidth="1001.0" style="-fx-font-size: 41px;" styleClass="label-zip">
            <font>
                <Font size="36.0" />
            </font>
         <graphic>
              <Label fx:id="statusLabel" style="-fx-font-size: 18;" styleClass="label-status" text="Resultados">
               <font>
                  <Font size="22.0" />
               </font>
            </Label>
         </graphic>
        </Label>
      <Button fx:id="btnBack" layoutX="58.0" layoutY="571.0" mnemonicParsing="false" onAction="#btnBackAction" prefHeight="45.0" prefWidth="182.0" styleClass="bt-blue-sm" text="Voltar">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="btnNext" disable="true" layoutX="539.0" layoutY="571.0" mnemonicParsing="false" onAction="#btnNextAction" prefHeight="45.0" prefWidth="182.0" styleClass="bt-blue-sm" text="Prosseguir">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button layoutX="295.0" layoutY="676.0" mnemonicParsing="false" styleClass="bt-cancelar" text="Cancelar" />
          <TextField fx:id="txtCep" layoutX="162.0" layoutY="204.0" prefHeight="60.0" prefWidth="700.0" promptText="CEP ou Endereço" styleClass="input-search">
              <font>
                  <Font size="18.0" />
              </font>
          </TextField>
          <Button fx:id="btnSearch" layoutX="787.0" layoutY="207.0" minHeight="-Infinity" mnemonicParsing="false" onAction="#btnSearchAction" prefHeight="23.0" prefWidth="73.0" styleClass="btn-search-img">
              <font>
                  <Font size="24.0" />
              </font>
          </Button>
      <Pane prefHeight="200.0" prefWidth="200.0" styleClass="header-page-service">
         <children>
            <Label layoutX="343.0" layoutY="56.0" styleClass="txt-label-service-teste" text="Postagem Nacional" />
         </children>
      </Pane>
      <Pane layoutY="110.0" prefHeight="200.0" prefWidth="200.0" styleClass="linha" />
      <Label layoutX="245.0" layoutY="142.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" style="-fx-font-size: 36px;" styleClass="label-remetente" text="Qual o endereço do Destinatário?">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Pane layoutX="73.0" layoutY="596.0" prefHeight="200.0" prefWidth="200.0" styleClass="seta-voltar" />
      <Pane layoutX="934.0" layoutY="596.0" prefHeight="200.0" prefWidth="200.0" styleClass="seta-next" />
    </children>
</AnchorPane>
